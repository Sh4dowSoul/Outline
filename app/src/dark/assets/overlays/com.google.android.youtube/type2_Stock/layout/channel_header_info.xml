<?xml version="1.0" encoding="utf-8"?>
<!-- Channel: Header Info -->
<GridLayout android:id="@*com.google.android.youtube:id/channel_header_info"
    android:background="@*android:color/background_dark"
    android:paddingTop="36.0dip"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:baselineAligned="false"
    android:rowCount="3"
    android:columnCount="5"
    android:columnOrderPreserved="false"
    _xmlns:android="http://schemas.android.com/apk/res/android"
    _xmlns:yt="http://schemas.android.com/apk/res-auto"
    xmlns:_xmlns="xmlns"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <com.google.android.libraries.youtube.common.ui.YouTubeTextView android:textSize="@*com.google.android.youtube:dimen/huge_font_size"
        android:textColor="#FFFFFF"
        android:ellipsize="end"
        android:gravity="start"
        android:layout_gravity="fill_horizontal"
        android:id="@*com.google.android.youtube:id/channel_title"
        android:paddingTop="12.0dip"
        android:layout_marginLeft="16.0dip"
        android:layout_marginRight="16.0dip"
        android:layout_column="0"
        android:maxLines="2"
        android:includeFontPadding="false"
        android:layout_row="0"
        android:layout_columnSpan="4"
        android:textAlignment="viewStart"
        res-auto:robotoFont="regular"
        xmlns:res-auto="http://schemas.android.com/apk/res-auto" />

    <ImageView
        android:id="@*com.google.android.youtube:id/profile_editor_icon"
        android:background="?selectableItemBackgroundBorderless"
        android:layout_width="48.0dip"
        android:layout_height="48.0dip"
        android:layout_marginRight="16.0dip"
        android:src="@*com.google.android.youtube:drawable/ic_settings_grey"
        android:scaleType="center"
        android:layout_column="4"
        android:contentDescription="@*com.google.android.youtube:string/channel_settings"
        android:layout_row="0"
        android:layout_marginEnd="16.0dip" />

    <include
        android:layout_gravity="center_vertical"
        android:id="@*com.google.android.youtube:id/subscribe_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8.0dip"
        android:layout_marginRight="8.0dip"
        android:layout_column="0"
        android:layout_row="1"
        layout="@*com.google.android.youtube:layout/subscribe_button" />

    <View
        android:layout_width="0.0dip"
        android:layout_marginLeft="8.0dip"
        android:layout_marginRight="8.0dip"
        android:layout_column="0"
        android:layout_row="1" />

    <com.google.android.libraries.youtube.common.ui.YouTubeTextView android:textSize="@*com.google.android.youtube:dimen/small_font_size"
        android:textColor="#b3FFFFFF"
        android:ellipsize="end"
        android:layout_gravity="center_vertical"
        android:id="@*com.google.android.youtube:id/channel_subtitle"
        android:layout_column="1"
        android:maxLines="1"
        android:singleLine="@*com.google.android.youtube:bool/rtl_single_line_true"
        android:layout_row="1"
        res-auto:robotoFont="regular"
        xmlns:res-auto="http://schemas.android.com/apk/res-auto" />

    <include
        android:layout_gravity="center_vertical"
        android:id="@*com.google.android.youtube:id/subscription_notification_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8.0dip"
        android:layout_column="2"
        android:layout_row="1"
        android:layout_marginStart="8.0dip"
        layout="@*com.google.android.youtube:layout/subscription_notification_button" />
</GridLayout>