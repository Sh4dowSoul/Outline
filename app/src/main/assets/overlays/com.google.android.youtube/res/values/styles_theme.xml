<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Settings - Light Statusbar -->
    <style name="Theme.YouTube.Settings" parent="@*com.google.android.youtube:style/Theme.YouTube.Light.NoActionBar">
        <item name="*com.google.android.youtube:colorPrimary">@*com.google.android.youtube:color/theme_main_color_primary</item>
        <item name="*com.google.android.youtube:colorPrimaryDark">@*com.google.android.youtube:color/theme_main_color_primary_dark</item>
        <item name="*com.google.android.youtube:colorAccent">@*com.google.android.youtube:color/material_deep_teal_500</item>
        <item name="*com.google.android.youtube:colorControlActivated">@*com.google.android.youtube:color/theme_main_color_accent</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>
